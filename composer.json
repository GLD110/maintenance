{
    "name": "stevebauman/maintenance",
    "description": "A Preventative Maintenance Application (CMMS) for Laravel",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman_7@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
                        "src/commands",
			"src/controllers",
			"src/models",
			"src/services",
			"src/validators",
			"src/exceptions",
			"src/filters",
			"src/composers",
			"src/apis",
			"src/requests",
                        "src/seeders"
        ],
        "psr-0": {
            "Stevebauman\\Maintenance\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
