{
    "name": "stevebauman/maintenance",
    "description": "A Preventative Maintenance Application (CMMS) for Laravel",
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.6",
        "illuminate/html": "5.0.0",
        "cartalyst/data-grid": "3.0.4",
        "cartalyst/sentry": "dev-feature/laravel-5",
        "stevebauman/corp": "1.0.8",
        "stevebauman/core-helper": "1.0.15",
        "stevebauman/inventory": "1.7.5",
        "stevebauman/calendar-helper": "0.1.5",
        "stevebauman/log-reader": "1.0.",
        "stevebauman/purify": "1.0.1",
        "stevebauman/revision": "1.1.0",
        "arcanedev/no-captcha": "2.0.4",
        "baum/baum": "1.1.1",
        "dompdf/dompdf": "0.6.1",
        "simplesoftwareio/simple-qrcode": "1.3.0",
        "davejamesmiller/laravel-breadcrumbs": "3.0.0",
        "google/apiclient": "1.1.4"
    },
    "repositories": [
    {
        "type": "composer",
        "url": "https://packages.cartalyst.com"
    }
    ],
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\Maintenance\\": "src/Stevebauman/Maintenance/"
        }
    },
    "autoload-dev":
    {
        "psr-4": {
            "Stevebauman\\Maintenance\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
